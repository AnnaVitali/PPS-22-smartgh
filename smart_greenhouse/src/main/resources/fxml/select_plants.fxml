<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css/baseStyle.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="50.0">
         <children>
            <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <center>
                  <ScrollPane hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="normalText">
                     <content>
                        <VBox fx:id="selectablePlantsBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" />
                     </content>
                  </ScrollPane>
               </center>
               <top>
                  <Label fx:id="selectYourPlantLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" BorderPane.alignment="TOP_LEFT">
                     <BorderPane.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </BorderPane.margin>
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </top>
            </BorderPane>
            <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <center>
                  <ScrollPane hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="normalText" BorderPane.alignment="CENTER">
                     <content>
                        <VBox fx:id="selectedPlantsBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" />
                     </content>
                  </ScrollPane>
               </center>
               <top>
                  <Label fx:id="plantsSelectedLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" BorderPane.alignment="TOP_LEFT">
                     <BorderPane.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </BorderPane.margin>
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </top>
               <bottom>
                  <HBox alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                     <BorderPane.margin>
                        <Insets top="20.0" />
                     </BorderPane.margin>
                     <children>
                        <Label fx:id="countLabel" contentDisplay="RIGHT" text="Label">
                           <font>
                              <Font name="System Bold" size="16.0" />
                           </font>
                        </Label>
                        <Label fx:id="numberPlantsSelectedLabel" text="Label">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </bottom>
            </BorderPane>
         </children>
      </HBox>
   </center>
   <bottom>
      <Label fx:id="errorLabel" styleClass="errorText" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
